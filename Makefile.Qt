VERSION = 5.3.3


#----------------USER SETTINGS-------------------------------

# 1. Where to install
#
PREFIX ?= /usr/local
bindir ?= $(PREFIX)/bin
libdir ?= $(PREFIX)/lib


# 2. Where moc and uic for QT4 is installed.
#
MOC ?= "`$(shell pwd)/find_moc_and_uic_paths.sh moc`"
UIC ?= "`$(shell pwd)/find_moc_and_uic_paths.sh uic`"
#RCC ?= "`./find_moc_and_uic_paths.sh rcc`"



# 4. Release buid or Debug build
#
BUILDTYPE ?= DEBUG
#BUILDTYPE=RELEASE

IS_LINUX_BINARY ?= 


# 5. Full path to a python2 executable.  (Think it needs to be at least V2.4)
#
PYTHONEXE ?= `which python2`



# 6. Opimization options. Only enabled in RELEASE build
OPTIMIZE ?= -O2


#----------------END USER SETTINGS---------------------------


GCC_VERSION:=$(shell gcc -dumpversion)
GCC_MAJOR:=$(shell echo $(GCC_VERSION) | cut -d'.' -f1)
GCC_MINOR:=$(shell echo $(GCC_VERSION) | cut -d'.' -f2)
GCC_SUB:=$(shell echo $(GCC_VERSION) | cut -d'.' -f3)




